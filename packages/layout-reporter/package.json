{
  "name": "@node-novel/layout-reporter",
  "version": "1.0.8",
  "description": "用來解析並且報告 txt 檔內的雜七雜八資訊",
  "keywords": [],
  "homepage": "https://github.com/bluelovers/node-novel#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/node-novel/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/node-novel.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "coverage": "npx nyc yarn run test",
    "lint": "npx eslint **/*.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "npm:publish": "npm publish",
    "npm:publish:lerna": "npx lerna publish --yes --cd-version patch",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "postpublish": "echo postpublish",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "ncu": "npx yarn-tool ncu -u",
    "sort-package-json": "npx yarn-tool sort",
    "tsc:default": "tsc -p tsconfig.json",
    "tsc:esm": "tsc -p tsconfig.esm.json"
  },
  "dependencies": {
    "array-chunk-split": "^1.0.6",
    "array-hyper-unique": "^1.1.10",
    "execall2": "^2.0.11",
    "json5": "^2.2.0",
    "string-natural-compare": "^3.0.1",
    "ts-type": "^1.2.39",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/json5": "^0.0.30"
  },
  "publishConfig": {
    "access": "public"
  }
}
